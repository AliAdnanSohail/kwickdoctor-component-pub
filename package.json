{
  "name": "kwickdoctor-components",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && yarn test:output && NODE_ENV=production `yarn bin`/babel ./src --out-dir ./dist",
    "storybook": "start-storybook -p 6006",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:build": "build-storybook",
    "test": "jest --coverage",
    "test:output": "yarn test --json --outputFile=.jest-test-results.json",
    "test:update": "yarn test --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamiaslam/kwickdoctor-components.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kamiaslam/kwickdoctor-components/issues"
  },
  "homepage": "https://github.com/kamiaslam/kwickdoctor-components#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!src/**/styles.js",
      "!dist/**",
      "!coverage/**"
    ]
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@storybook/addon-links": "3.3.15",
    "@storybook/addon-storyshots": "3.3.15",
    "@storybook/addons": "3.3.15",
    "@storybook/storybook-deployer": "2.3.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-sass-export": "^0.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
