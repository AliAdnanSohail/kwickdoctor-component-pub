  },
  "dependencies": {
    "@storybook/addon-a11y": "3.3.15",
    "@storybook/addon-actions": "3.3.15",
    "@storybook/addon-info": "3.3.15",
    "@storybook/addon-jest": "3.3.15",
    "@storybook/addon-knobs": "3.3.15",
    "@storybook/addon-notes": "3.3.15",
    "@storybook/react": "3.3.15",
    "classnames": "^2.2.5",
    "grommet": "^1.10.1",
    "moment": "^2.21.0",
    "moment-range": "^3.1.1",
    "react-aria-modal": "^2.12.1",
    "react-datepicker": "^1.2.2",
    "react-motion": "^0.5.2",
