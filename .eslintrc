{
  "rules": {
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "indent": ["error", 2],
    "react/jsx-indent": ["error", 2],
    "react/jsx-indent-props": ["error", 2],
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/label-has-for": [ 2, {
      "components": [ "Label" ],
      "required": {
          "some": [ "nesting", "id" ]
      },
      "allowChildren": false
    }]
  },
  "extends": ["airbnb"],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "globals": {
    "google": true,
    "describe": true,
    "it": true,
    "expect": true
  }
}
